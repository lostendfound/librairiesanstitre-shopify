{
  "name": "libr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"SHOPIFY,TAILWIND\" \"shopify theme dev --store librairie-sans-titre-2\" \"npx @tailwindcss/cli -i ./style.css -o ./assets/style-vite.css --watch\"",
    "vite:dev": "vite dev",
    "tailwind:dev": "npx @tailwindcss/cli -i ./style.css -o ./assets/style-vite.css --watch",
    "tailwind:build": "npx @tailwindcss/cli -i ./style.css -o ./assets/style-vite.css",
    "shopify:dev": "shopify theme dev --store librairie-sans-titre-2",
    "shopify:build": "shopify theme build --store librairie-sans-titre-2",
    "vite:build": "vite build",
    "vite:build:watch": "vite build --watch",
    "push": "shopify theme push --ignore ./docs/*",
    "pull": "shopify theme pull --ignore ./docs/*",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port $PORT --host"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "chokidar": "^5.0.0",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.0",
    "vitepress": "^1.6.4"
  },
  "dependencies": {
    "html2canvas": "^1.4.1"
  }
}
